<% include ./partials/header.ejs%>
<section id="contact">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-6">
                <div class="contact-image-wrapper">
                    <img src="./images/2686827.jpg" alt="">
                </div>
            </div>
            <div class="col-md-6">
                <div class="contact-wrapper mx-auto">
                    <h2 class="color-pri mb-4 head-contact">Send Your Message</h2>
                    <form action="">
                        <div class="mb-3">
                            <label for="">Name</label>
                            <input class="form-control" type="text">
                        </div>
                        <div class="mb-3">
                            <label for="">Email</label>
                            <input class="form-control" type="email">
                        </div>
                        <div class="mb-3">
                            <label for="">Phone</label>
                            <input class="form-control" type="text">
                        </div>
                        <div class="mb-3">
                            <label for=""></label>
                            <textarea class="form-control" rows="5"placeholder="type something here"></textarea>
                        </div>
                        <button type="submit" class="btn bg-color-pr text-white">Send Message</button>
                        
                    </form>
                </div>
            </div>
        </div>
    </div>
</section>
